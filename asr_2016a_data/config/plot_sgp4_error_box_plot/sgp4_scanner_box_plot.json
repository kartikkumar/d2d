{
    // Path to SQLite databases containing scan data.
    "leo_database"                  : "./SSO/sso_debris.db",
    "heo_database"                  : "./HEO/heo_rocket_bodies.db",
    "geo_database"                  : "./GEO/geo_intelsat.db",

    // Relative path to output directory where the plots will be saved.
    // Do not include a '/' at the end.
    "output_directory"              : "./figures",

    // Set name for the figure
    "figure_name"                   : "Figure_9_box_plot",

    // Set output format for the figure (example: ".pdf", ".png")
    "figure_format"                 : ".png",

    // Check for GUI
    // If user machine has a display/GUI then put True, else False
    "display"                       : "True"
}
